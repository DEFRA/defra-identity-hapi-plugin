{
  "name": "defra.identity-hapi-plugin",
  "version": "0.0.1",
  "description": "A hapi auth plugin to allow easy integration with DEFRA's Identity Management system",
  "main": "./lib/index.js",
  "scripts": {
    "test": "npm run lint; ./node_modules/.bin/lab -v",
    "lint": "(./node_modules/standard/bin/cmd.js --verbose | ./node_modules/snazzy/bin/cmd.js) && echo 'All lint rules passed' || true",
    "demo": "DEBUG=defra.identity:* node demo",
    "postinstall": "cp -n demo/.env.template demo/.env || echo 'Note: Error creating demo/.env. File may already exist'"
  },
  "author": "Chris Cheshire",
  "license": "ISC",
  "engines": {
    "node": "8.9.1",
    "npm": "5.7.1"
  },
  "dependencies": {
    "await-to-js": "^2.0.1",
    "debug": "^3.1.0",
    "hapi-auth-cookie": "^8.1.0",
    "openid-client": "^1.20.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "code": "^5.2.0",
    "lab": "^15.4.1",
    "snazzy": "^7.1.1",
    "standard": "^11.0.1",
    "blipp": "^3.0.0",
    "catbox-mongodb": "^4.2.0",
    "dotenv": "^5.0.1",
    "ejs": "^2.5.7",
    "hapi": "^17.2.1",
    "inert": "^5.1.0",
    "vision": "^5.3.1"
  }
}
